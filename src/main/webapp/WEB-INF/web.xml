<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginCheck</servlet-name>
  	<display-name>LoginCheck</display-name>
  	<description></description>
  	<servlet-class>com.simplilearn.servlets.admin.LoginCheck</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SignCheck</servlet-name>
  	<display-name>SignCheck</display-name>
  	<description></description>
  	<servlet-class>com.simplilearn.servlets.users.LoginCheck</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SigninCheck</servlet-name>
  	<display-name>SigninCheck</display-name>
  	<description></description>
  	<servlet-class>com.simplilearn.servlets.admin.LoginCheck</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginCheck</servlet-name>
  	<url-pattern>/LoginCheck</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SignCheck</servlet-name>
  	<url-pattern>/SignCheck</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SigninCheck</servlet-name>
  	<url-pattern>/SigninCheck</url-pattern>
  </servlet-mapping>
</web-app>
